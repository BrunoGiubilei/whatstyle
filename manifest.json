{
  "manifest_version": 3,
  "version": "1.1",
  "name": "Whatstyle",
  "description": "Project made to customize the web version of WhatsApp",
  "author": "Bruno Giubilei",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [{
    "css": ["style.css"],
    "js": ["style.js"],
    "matches": ["https://web.whatsapp.com/*"]
  }],
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}